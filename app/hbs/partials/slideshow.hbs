<ul class="pp-slideshow"
	tracking="{{title}}"
	data-slick='{
		"dots":true,
		"infinite":true,
        "accessibility":false,
        "slidesToShow": 1,
        "slidesToScroll": 1
	}'>
	{{#each images }}
	<li class="slide">
		<img src="{{ image }}"/>
		<div class="slide-caption">
			<p>{{{ paragraphs }}}</p>
		</div>
	</li>
	{{/each}}
</ul>
